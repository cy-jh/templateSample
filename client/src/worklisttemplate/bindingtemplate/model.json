{
	"neoapp": {
		"welcomeFile": "/webapp/index.html",
		"sendWelcomeFileRedirect": true
	},
	"basicSAPUI5ApplicationProject": {
		"parameters": {
			"ViewTypesCollection": {
				"type": "String",
				"binding": [{
					"name": "None",
					"value": "None"
				}, {
					"name": "XML",
					"value": "XML"
				}, {
					"name": "JSON",
					"value": "JSON"
				}, {
					"name": "JavaScript",
					"value": "JS"
				}, {
					"name": "HTML",
					"value": "HTML"
				}],
				"value": "@basicSAPUI5ApplicationProject.parameters.ViewTypesCollection.binding.1",
				"wizard": {
					"control": "ComboBox",
					"required": true,
					"title": "basicSAPUI5ApplicationProject_view_type_title",
					"tooltip": "basicSAPUI5ApplicationProject_view_type_tooltip",
					"sort": "false"
				}
			},
			"namespace": {
				"type": "string",
				"value": "",
				"appDescriptor": {
					"id": "Namespace"
				},
				"wizard": {
					"control": "TextField",
					"required": true,
					"title": "basicSAPUI5ApplicationProject_namespace_title",
					"regExp": "(?!(sap|new(\\..*)*)$)^[a-zA-Z_][\\w-]*(\\.[a-zA-Z_][\\w-]*)*$",
					"regExpErrMsg": "nameSpace_model_parameters_name_validationError"
				}
			},
			"name": {
				"type": "string",
				"value": "MainView",
				"wizard": {
					"control": "TextField",
					"required": false,
					"title": "basicSAPUI5ApplicationProject_view_name_title",
					"regExp": "^[a-zA-Z_]+[a-zA-Z0-9\\-_]*$",
					"regExpErrMsg": "viewXML_model_parameters_name_validationError"
				}
			},
			"isSearchPanel": {
				"type": "string",
				"binding": [{
					"name": "Yes",
					"value": "true"
				}, {
					"name": "No",
					"value": "false"
				}],
				"value": "@basicSAPUI5ApplicationProject.parameters.isSearchPanel.binding.0",
				"wizard": {
					"control": "ComboBox",
					"required": true,
					"title": "needs for search panel"
				}
			},
			"iSearchSetCount1": {
				"type": "string",
				"binding": [{
					"name": "Yes",
					"value": "true"
				}, {
					"name": "No",
					"value": "false"
				}],
				"value": "@basicSAPUI5ApplicationProject.parameters.iSearchSetCount1.binding.1",
				"wizard": {
					"control": "ComboBox",
					"required": false,
					"title": "needs for SearchSet1",
					"regExpErrMsg": "viewXML_model_parameters_name_validationError"
				}
			},
			"iSearchSetCount2": {
				"type": "string",
				"binding": [{
					"name": "Yes",
					"value": "true"
				}, {
					"name": "No",
					"value": "false"
				}],
				"value": "@basicSAPUI5ApplicationProject.parameters.iSearchSetCount2.binding.0",
				"wizard": {
					"control": "ComboBox",
					"required": false,
					"title": "needs for SearchSet2",
					"regExpErrMsg": "viewXML_model_parameters_name_validationError"
				}
			},
			"popupYN" : true,
			"popupName" : ""
		},
		"forms": [{
			"title": "",
			"type": "templateCustomizationStep",
			"groups": [{
				"title": "emptyMobileProject_model_form_group_title",
				"parameters": ["@basicSAPUI5ApplicationProject.parameters.ViewTypesCollection", "@basicSAPUI5ApplicationProject.parameters.name",
				"@basicSAPUI5ApplicationProject.parameters.isSearchPanel", "@basicSAPUI5ApplicationProject.parameters.iSearchSetCount1", "@basicSAPUI5ApplicationProject.parameters.iSearchSetCount2"
				]
			}]
		}, {
			"type": "appDescriptorGenericStep",
			"groups": [{
				"parameters": ["@basicSAPUI5ApplicationProject.parameters.namespace"]
			}]
		}]
	}
}